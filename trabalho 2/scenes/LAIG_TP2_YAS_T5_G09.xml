<yas>
    <scene root="scenario" axis_length="5.0" />

    <views default="Default" >
        <perspective id="Default" near="0.1" far="500" angle="45">
            <from x="100" y="100" z="100" />
            <to x="0" y="-2" z="0" />
        </perspective>
        <perspective id="Table" near="0.1" far="600" angle="40">
            <from x="25" y="10" z="12" />
            <to x="9" y="2" z="11" />
        </perspective>
        <ortho id="Ortho" near="0.4" far="200" left="-15" right="15" top="12" bottom="-6" >
            <from x="30" y="5" z="25" />
            <to x="0" y="5" z="3" />
        </ortho>
    </views>

    <ambient>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.7" g="0.9" b="0.96" a="1.0" />
    </ambient>

    <lights>
        <omni id="Candeeiro" enabled="1" >
        <location x="9.8" y="11.9" z="10" w="1.0" />
        <ambient r="0" g="0" b="0" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>

        <spot id="Spot Light" enabled="0" angle="0.0" exponent="0.0">
            <location x="10.2" y="11.9" z="10" w="1.0" />
            <target x="5.0" y="0.0" z="5.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </spot>

        <omni id="Ambiente" enabled="1" >
        <location x="13.0" y="20.0" z="13.0" w="1.0" />
        <ambient r="0" g="0" b="0" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>

        <omni id="1" enabled="1" >
        <location x="-7.0" y="2.0" z="3.0" w="1.0" />
        <ambient r="0" g="0" b="0" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>

        <omni id="2" enabled="1" >
        <location x="0.0" y="-5.0" z="3.0" w="1.0" />
        <ambient r="0" g="0" b="0" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
        <omni id="3" enabled="1" >
        <location x="7.0" y="2.0" z="3.0" w="1.0" />
        <ambient r="0" g="0" b="0" a="1.0" />
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
    </lights>

    <textures>
        <texture id="terrain" file="scenes/images/terrain.jpg"/>
        <texture id="terrainMap" file="scenes/images/heightmap.jpg"/>
        <texture id="water" file="scenes/images/water.jpg"/>
        <texture id="waterMap" file="scenes/images/waterHeightMap.jpg"/>
        <texture id="bridge" file="scenes/images/bridge.jpg"/>
        <texture id="heliport" file="scenes/images/heliport.jpg"/>
        <texture id="stripes" file="scenes/images/stripes.png"/>
    </textures>

    <materials>
        <material id="white" shininess = "30" >
            <emission r="0" g="0" b="0" a="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1" g="1" b="1" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </material>

        <material id="red" shininess = "10" >
            <emission r="0" g="0" b="0" a="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.5" g="0.1" b="0.1" a="1.0" />
            <specular r="0.6" g="0" b="0" a="1.0" />
        </material>

        <material id="grey" shininess = "50" >
            <emission r="0" g="0" b="0" a="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.3" g="0.3" b="0.3" a="1.0" />
            <specular r="0.3" g="0.3" b="0.3" a="1.0" />
        </material>

        <material id="blue" shininess = "50" >
            <emission r="0" g="0" b="0.2" a="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.1" g="0.1" b="1" a="1.0" />
            <specular r="0" g="0" b="1" a="1.0" />
        </material>

        <material id="black" shininess = "50" >
            <emission r="0" g="0" b="0" a="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.1" g="0.1" b="0.1" a="1.0" />
            <specular r="0.1" g="0.1" b="0.1" a="1.0" />
        </material>

        <material id="black_glass" shininess = "500" >
            <emission r="0" g="0" b="0" a="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0" g="0" b="0" a="1.0" />
            <specular r="1" g="1" b="1" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="terrain">
            <translate x="-5" y="-2" z="-5" />
            <scale x="50" y="1" z="50" />
        </transformation>
        <transformation id="water">
            <scale x="100" y="1" z="100" />
            <translate x="0" y="-0.5" z="0" />
        </transformation>
    </transformations>

    <animations>
        <linear id="unstick" span="5">
          <controlpoint xx="0" yy="0" zz="0"/>
          <controlpoint xx="0" yy="20" zz="7"/>
        </linear>
        <circular id="rote" span="3" center="-15 20 7" radius="15" startang="0" rotang="-90"/>
        <circular id="anim" span="4" center="0 0 5" radius="10" startang="0" rotang="-90"/>
        <linear id="anim_1" span="4">
            <controlpoint xx="-15" yy="20" zz="22"/>
            <controlpoint xx="-50" yy="20" zz="22"/>
        </linear>
    </animations>

    <primitives>
        <!--   PRIMITIVAS NURBS    -->
        <primitive id="plane">
            <plane npartsU="30" npartsV="30" />
        </primitive>

        <primitive id="patch1">
            <patch npointsU="3" npointsV="2" npartsU="20" npartsV="30">
                <controlpoint xx="6.5" yy="8" zz="9.5"/>
                <controlpoint xx="9.5" yy="8" zz="9.5"/>
                <controlpoint xx="7.5" yy="5.5" zz="15"/>
                <controlpoint xx="10.5" yy="5.5" zz="15"/>
                <controlpoint xx="8.5" yy="7" zz="24"/>
                <controlpoint xx="11.5" yy="7" zz="24"/>
            </patch>
        </primitive>

        <primitive id="patch2">
            <patch npointsU="3" npointsV="2" npartsU="20" npartsV="30">
                <controlpoint xx="8.5" yy="7" zz="24"/>
                <controlpoint xx="11.5" yy="7" zz="24"/>
                <controlpoint xx="7.5" yy="5.5" zz="15"/>
                <controlpoint xx="10.5" yy="5.5" zz="15"/>
                <controlpoint xx="6.5" yy="8" zz="9.5"/>
                <controlpoint xx="9.5" yy="8" zz="9.5"/>
            </patch>
        </primitive>

        <primitive id="vehicle">
            <vehicle/>
        </primitive>

        <primitive id="cylinder">
            <cylinder2  base="1" top="1" height="1" slices="25" stacks="20" />
        </primitive>

        <primitive id="cylinder_covered">
            <cylinder  base="1.2" top="1" height="1" slices="30" stacks="20" />
        </primitive>

        <primitive id="cut_cone">
            <cylinder2  base="1" top="2" height="0.5" slices="25" stacks="20" />
        </primitive>

        <primitive id="cone">
            <cylinder base="1" top="0" height="1" slices="30" stacks="20" />
        </primitive>

        <primitive id="terrain">
            <terrain  idtexture="terrain" idheightmap="terrainMap" parts="50" heightscale="10" />
        </primitive>

        <primitive id="water">
            <water  idtexture="water" idwavemap="waterMap" parts="50" heightscale="2" texscale="3" />
        </primitive>
    </primitives>

    <components>
        <component id="water">
            <transformation>
                <transformationref id="water" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="water" />
            </children>
        </component>

        <component id="terrain">
            <transformation>
                <transformationref id="terrain" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="terrain" />
            </children>
        </component>

        <component id="vehicle">
            <transformation>
                <rotate axis="y" angle="-90" />
                <translate x="0" y="8" z="-2" />
            </transformation>
            <animations>
                <animationref id="unstick" />
                <animationref id="rote" />
                <animationref id="anim_1" />
            </animations>
            <materials>
                <material id="blue" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="vehicle" />
            </children>
        </component>

        <component id="heliport_base">
            <transformation>
                <scale x="1" y="6.5" z="1" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>

        <component id="heliport_join">
            <transformation>
                <translate x="0" y="6.5" z="0" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="cut_cone" />
            </children>
        </component>

        <component id="heliport_platf">
            <transformation>
                <translate x="0" y="7" z="0" />
                <scale x="8" y="1" z="8" />
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="plane" />
            </children>
        </component>
        <component id="heliport_platf_versus">
            <transformation>
                <translate x="0" y="7" z="0" />
                <scale x="8" y="1" z="8" />
                <rotate axis="x" angle="180" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="plane" />
            </children>
        </component>

        <component id="heliport">
            <transformation>
                <translate x="10" y="0" z="28" />
            </transformation>
            <materials>
                <material id="grey" />
            </materials>
            <texture id="heliport" length_s="1" length_t="1" />
            <children>
                <componentref id="heliport_base" />
                <componentref id="heliport_join" />
                <componentref id="heliport_platf" />
                <componentref id="heliport_platf_versus" />
                <componentref id="vehicle" />
            </children>
        </component>

        <component id="bridge">
            <transformation></transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="bridge" length_s="1" length_t="1" />
            <children>
                <primitiveref id="patch1" />
                <primitiveref id="patch2" />
            </children>
        </component>

        <component id="walls">
            <transformation>
                <scale x="2" y="18" z="2" />
                <rotate axis="x" angle="-90" />
                <rotate axis="z" angle="90" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" />
            <children>
                <primitiveref id="cylinder_covered" />
            </children>
        </component>

        <component id="light">
            <transformation>
                <translate x="0" y="18" z="0" />
                <scale x="1.4" y="2" z="1.4" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="cylinder" />
            </children>
        </component>

        <component id="roof">
            <transformation>
                <translate x="0" y="20" z="0" />
                <scale x="1.6" y="1" z="1.6" />
                <rotate axis="x" angle="-90" />
            </transformation>
            <materials>
                <material id="red" />
            </materials>
            <texture id="none" />
            <children>
                <primitiveref id="cone" />
            </children>
        </component>

        <component id="lightHouse">
            <transformation>
                <translate x="8" y="2" z="8" />
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="stripes" length_s="1" length_t="1" />
            <children>
                <componentref id="walls" />
                <componentref id="light" />
                <componentref id="roof" />
            </children>
        </component>

        <component id="scenario">
            <transformation></transformation>
            <materials>
                <material id="default" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="terrain" />
                <componentref id="water" />
                <componentref id="lightHouse" />
                <componentref id="bridge" />
                <componentref id="heliport" />
            </children>
        </component>
    </components>
</yas>
